{
    "version": 2,
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["src/**/*.js", "package.json", "node_modules/**/*"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/index.js"
      }
    ],
    "public": true
  }
  